{
  "name": "executable-stories-jest",
  "version": "3.1.0",
  "description": "BDD-style executable stories for Jest with documentation generation",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./reporter": {
      "types": "./dist/reporter.d.ts",
      "import": "./dist/reporter.js",
      "require": "./dist/reporter.cjs"
    },
    "./setup": {
      "types": "./dist/setup.d.ts",
      "import": "./dist/setup.js",
      "require": "./dist/setup.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.mjs --passWithNoTests"
  },
  "peerDependencies": {
    "jest": ">=30.2.0",
    "executable-stories-formatters": "^0.2.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "executable-stories-formatters": "workspace:*",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagreehal/executable-stories.git",
    "directory": "packages/executable-stories-jest"
  },
  "keywords": [
    "jest",
    "bdd",
    "testing",
    "documentation",
    "executable-stories"
  ]
}
