{
  "name": "executable-stories-playwright",
  "version": "3.1.0",
  "description": "BDD-style executable stories for Playwright Test with documentation generation",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./reporter": {
      "types": "./dist/reporter.d.ts",
      "import": "./dist/reporter.js",
      "require": "./dist/reporter.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "playwright test --pass-with-no-tests"
  },
  "peerDependencies": {
    "@playwright/test": ">=1.58.2",
    "executable-stories-formatters": "^0.2.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/node": "^25.2.3",
    "executable-stories-formatters": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagreehal/executable-stories.git",
    "directory": "packages/executable-stories-playwright"
  },
  "keywords": [
    "playwright",
    "bdd",
    "testing",
    "documentation",
    "executable-stories"
  ]
}
